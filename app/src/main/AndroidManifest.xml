<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.galaxygang_marsmadness_round2">


    <uses-feature android:name="android.hardware.camera"
        android:required="false" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- implicitly allows all external storage to write the data, and read is included -->



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <!--specify an intent filter in the manifest for deep linking, now indexable by Google search -->
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>
        <activity android:name=".Camera"></activity>
        <activity android:name=".homeActivity"></activity>
        <activity android:name=".cardsMenu"></activity>
        <activity android:name=".multiMenu"></activity>
        <activity android:name=".gameActivity"></activity>
        <activity android:name=".WiFisearch"></activity>
        <activity android:name=".nameActivity"></activity>



    </application>

</manifest>